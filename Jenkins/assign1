pipeline {
agent any

stages {
stage ('CHECKOUT') {
steps { 
     checkout scmGit(branches: [[name: '*/main']],
     extensions: [],
     userRemoteConfigs: [[credentialsId: 'github_acc', url: 'https://github.com/sachingowda0159/my-first-repo.git']])
}
}

stage ('CHECK') {
when {
branch 'main'
  }
}

stage ('BUILD') {

steps {
sh ' pwd '

        }
       }

stage ('TEST') {
steps {
sh ' echo "Hii hello" '

      }
     }
    }
   }
