pipeline {
agent any

stages {
stage ('CHECK') {
when {
branch 'main'
}
steps {
git credentialsId: 'github_acc', url: 'https://github.com/sachingowda0159/my-first-repo.git'
}
}

stage ('BUILD') {
when {
branch 'main'
}
steps {
sh ' pwd '

        }
       }

stage ('TEST') {
steps {
sh ' echo "Hii hello" '

      }
     }
    }
   }
