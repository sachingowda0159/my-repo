pipeline {
agent any

stages {
stage ('CHECK') {
when {
branch 'main'
}
steps { 
       checkout([$class: 'GitSCM',
       userRemoteConfigs: [[url: 'https://github.com/sachingowda0159/my-first-repo.git',
       branches: [[name: '*/main']],
       credentialsId: 'github_acc']]])
}
}

stage ('BUILD') {
steps {
sh ' pwd '

        }
       }

stage ('TEST') {
steps {
sh ' echo "Hii hello" '

      }
     }
    }
   }
